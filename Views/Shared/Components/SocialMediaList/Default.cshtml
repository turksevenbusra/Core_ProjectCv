@using EntityLayer.Concrete
@model List<SocialMedia>

<div class="social-nav" data-aos="fade-up" data-aos-delay="200">
    <nav role="navigation">
        <ul class="nav justify-content-left">
            @foreach (var sm in Model)
            {
                <li class="nav-item">
                    <a class="nav-link" href="@sm.Url" title="Twitter">
                        <i class="@sm.Icon"></i>
                        <span class="menu-title sr-only">
                            @sm.Name
                        </span>
                    </a>
                </li>
            }
        </ul>
    </nav>
</div>